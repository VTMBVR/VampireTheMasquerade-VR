"LightmappedGeneric"
{
	"$baseTexture" "water/Bloody_Water"
	"$bumpmap"			"dev/water_normal"
	"$envmap"			"envmap/andrei_battle"
	"$envmapmask" "effects/ref_50"
	"$bumpframe"			"0"
	"$bumpoffset"			"[0 0]"
	"$envmaptint" "[.2 0 0]"
	"$scale" "[0.5 0.5]"
//	"%compilewater" 1

	"$surfaceprop" "water"


	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 20.00
		}

		"TextureScroll"
		{
			"texturescrollvar" "$bumpoffset"
			"texturescrollrate" .02
			"texturescrollangle" 45.00
		}
	}
}
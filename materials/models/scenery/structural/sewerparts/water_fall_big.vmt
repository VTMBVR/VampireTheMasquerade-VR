"VertexLitGeneric"
{
	"$baseTexture" "models/scenery/structural/SewerParts/water_fall_small"

//	"$bumpmap"			"dev/water_dudv"
	"$bumpmap"			"dev/water_normal2"
	"$bumpframe"			"0"
	"$bumpoffset"			"[0 0]"
	"$envmap"			"envmap/sewer"
	"$normalmapalphaenvmapmask"	"1"

	"$translucent"			"1"
	"$alpha"			"1"
	"$nocull"			"1"


	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
		
		"TextureScroll"
		{
			"texturescrollvar" "$bumpoffset"
			"texturescrollrate" 1
			"texturescrollangle" 90
		}

		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" 1
			"texturescrollangle" 90
		}


	}

}
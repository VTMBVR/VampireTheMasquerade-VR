VertexLitGeneric
{
	"$basetexture"	"models\scenery\structural\warrens\warr01_waterslide"
//	"$bumpmap"			"dev/water_dudv"
	"$bumpmap"			"models\scenery\structural\warrens\warr01_waterslide_normal"
//	"$bumpmap"			"dev/water_normal2"
	"$bumpframe"			"0"
	"$bumpoffset"			"[0 0]"
	"$envmap"			"envmap/sewer"
//	"$envmap"			"env_cubemap"
	"$normalmapalphaenvmapmask"	"1"

	"$translucent"			"1"
	"$alpha"			"1"
	"$nocull"			"1"


	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
		
		"TextureScroll"
		{
			"texturescrollvar" "$bumpoffset"
			"texturescrollrate" -1.5
			"texturescrollangle" 90
		}

		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" -.5
			"texturescrollangle" 90
		}


	}

}